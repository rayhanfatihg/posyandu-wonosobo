generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model LayananLansia {
  wargaId       String?
  beratBadan    Float?
  tinggiBadan   Float?
  lingkarPerut  Float?
  lingkarLengan Float?
  createdAt     DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt     DateTime? @updatedAt @db.Timestamp(6)
  tensiDarah    String?
  id            String?   @default(uuid())
  warga         warga?    @relation(fields: [wargaId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@ignore
}

model LayananAnak {
  id                String    @id @default(uuid())
  anakId            String?   @unique @db.VarChar
  anak              String?
  tinggiBadanAnak   Float?    @db.Real
  beratBadanAnak    Float?    @db.Real
  lingkarKepalaAnak Float?    @db.Real
  lingkarLenganAnak Float?    @db.Real
  createdAt         DateTime? @default(now()) @db.Timestamp(6)
  updatedAt         DateTime? @updatedAt @db.Timestamp(6)
  warga             warga?    @relation(fields: [anakId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model blog {
  id           String        @id @default(uuid())
  title        String?
  image_url    String?
  is_premium   Boolean?      @default(false)
  is_published Boolean?      @default(true)
  createdAt    DateTime?     @default(now()) @db.Timestamp(6)
  blog_content blog_content?
}

model blog_content {
  blog_id   String    @id @default(uuid())
  content   String?
  createdAt DateTime? @default(now()) @db.Timestamp(6)
  blog      blog      @relation(fields: [blog_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model JadwalPosyandu {
  id        String    @id @default(uuid())
  namaAcara String?
  tanggal   DateTime? @db.Timestamp(6)
  jam       String?
  createdAt DateTime? @default(now()) @db.Timestamp(6)
  updatedAt DateTime? @updatedAt @db.Timestamp(6)
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model warga {
  id            String          @id
  nama          String
  nik           String
  tanggallahir  DateTime        @db.Date
  umur          Int
  createdAt     DateTime        @default(now()) @db.Timestamp(6)
  updatedAt     DateTime        @db.Timestamp(6)
  LayananAnak   LayananAnak?
  LayananLansia LayananLansia[] @ignore
}
