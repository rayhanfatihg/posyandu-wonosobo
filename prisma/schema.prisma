generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model LayananLansia {
  id           String   @id @default(uuid())
  wargaId      String
  beratBadan   Float?
  tinggiBadan  Float?
  asamUrat     Float?
  gulaDarah    Float?
  keterangan   String?
  kolesterol   Float?
  lingkarPerut Float?
  tensiDarah   String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  warga        Warga    @relation(fields: [wargaId], references: [id])
}

model blog {
  id           String        @id @default(uuid())
  createdAt    DateTime      @default(now())
  title        String
  image_url    String
  is_premium   Boolean       @default(false)
  is_published Boolean       @default(true)
  blog_content blog_content?
}

model blog_content {
  blog_id   String   @id @default(uuid())
  content   String
  createdAt DateTime @default(now())
  blog      blog     @relation(fields: [blog_id], references: [id])
}

model JadwalPosyandu {
  id        String   @id @default(uuid())
  namaAcara String
  tanggal   DateTime
  jam       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model LayananIbuAnak {
  id                                 String       @id
  tinggiBadanAnak                    Float?
  beratBadanAnak                     Float?
  umurAnak                           Float?
  lingkarLenganAnak                  Float?
  lingkarKepalaAnak                  Float?
  tinggiBadanIbu                     Float?
  beratBadanIbu                      Float?
  lingkarLenganIbu                   Float?
  lingkarPinggangIbu                 Float?
  alatKontrasepsi                    String?
  anakId                             String
  ayahId                             String
  ibuId                              String
  jenisKelaminAnak                   JenisKelamin
  createdAt                          DateTime     @default(now())
  updatedAt                          DateTime
  Warga_LayananIbuAnak_anakIdToWarga Warga        @relation("LayananIbuAnak_anakIdToWarga", fields: [anakId], references: [id])
  Warga_LayananIbuAnak_ayahIdToWarga Warga        @relation("LayananIbuAnak_ayahIdToWarga", fields: [ayahId], references: [id])
  Warga_LayananIbuAnak_ibuIdToWarga  Warga        @relation("LayananIbuAnak_ibuIdToWarga", fields: [ibuId], references: [id])
}

model Warga {
  id                                          String           @id
  nama                                        String
  nik                                         String           @unique
  tanggalLahir                                DateTime
  umur                                        Int
  createdAt                                   DateTime         @default(now())
  updatedAt                                   DateTime
  dusun                                       String?
  LayananIbuAnak_LayananIbuAnak_anakIdToWarga LayananIbuAnak[] @relation("LayananIbuAnak_anakIdToWarga")
  LayananIbuAnak_LayananIbuAnak_ayahIdToWarga LayananIbuAnak[] @relation("LayananIbuAnak_ayahIdToWarga")
  LayananIbuAnak_LayananIbuAnak_ibuIdToWarga  LayananIbuAnak[] @relation("LayananIbuAnak_ibuIdToWarga")
  LayananLansia                               LayananLansia[]
}

enum JenisKelamin {
  LAKI_LAKI
  PEREMPUAN
}
